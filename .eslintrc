{
  "parser": "@babel/eslint-parser",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "require-jsdoc" : 0,
    "operator-linebreak": "off"
  },
  "parserOptions": {
    "requireConfigFile": false
  },
  "plugins": ["jest"],
  "extends": ["eslint:recommended", "google"]
}
